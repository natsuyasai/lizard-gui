{
  "extends": "../../tsconfig.web.json",
  "include": [
    "../../src/renderer/src/env.d.ts",
    "../../src/renderer/src/**/*",
    "../../src/renderer/src/**/*.tsx",
    "../../src/preload/*.d.ts",
    "../../src/types/types.ts", 
    "../../test/renderer/**/*"  ],
  "compilerOptions": {
    "rootDir": "../../",
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": "../../",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ],
      "@/*": ["src/*"]
    },
    "types":["vitest/globals"]
  }
}
